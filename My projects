print ("Welcome to place the rabbit")
tree = [["🌲", "🌲", "🌲", ], ["🌲", "🌲", "🌲"], ["🌲", "🌲", "🌲", ]]
print (f"{tree[0]} \n{tree[1]} \n{tree [2]}")
print ("Where should the rabbit go? 🐇")
position =  input("Please, choose a row and a column: \n")
row = int(position [0])
column = int(position [1])
tree [row-1][column-1] = "🐇"
print ("Success.....")
print (f"{tree[0]} \n{tree[1]} \n{tree [2]}")